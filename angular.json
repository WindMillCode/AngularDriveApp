{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"AngularApp": {
			"projectType": "application",
			"schematics": {},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"drive": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/Drive",
						"baseHref": "",
						"fileReplacements": [
							{
								"replace": "src/app/website.ts",
								"with": "src/app/database/website-drive-dev.ts"
							},
							{
								"replace": "src/environments/environment.ts",
								"with": "src/environments/environment.drive.dev.ts"
							}
						],
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": true,
						"assets": [],
						"styles": [
							"src/styles.css",
							"src/assets/styles/pikaday.css"
						],
						"scripts": [
							"backend/modernizr/modernizr.js",
							"backend/polyfills/object.js",
							"backend/polyfills/signaturePad.js",
							"src/assets/scripts/pikaday.js"
						]
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.drive.prod.ts"
								},
								{
									"replace": "src/app/website.ts",
									"with": "src/app/database/website-drive-prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"assets": []
						},
						"development": {
							"optimization": false,
							"sourceMap": false,
							"poll": 2500,
							"verbose": false,
							"watch": true
						},
						"playground": {
							"fileReplacements": [
								{
									"replace": "src/app/website.ts",
									"with": "src/app/database/website-playground-dev.ts"
								},
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.drive.dev.ts"
								}								
							],							
							"optimization": false,
							"sourceMap": true,
							"poll": 2500,
							"verbose": false,
							"watch": true
						},						
						"upload": {
							"fileReplacements": [
								{
									"replace": "src/app/website.ts",
									"with": "src/app/database/website-upload-dev.ts"
								},
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.drive.dev.ts"
								}								
							],							
							"optimization": false,
							"sourceMap": false,
							"poll": 2500,
							"verbose": false,
							"watch": true
						},
						"folders": {
							"fileReplacements": [
								{
									"replace": "src/app/website.ts",
									"with": "src/app/database/website-folders-dev.ts"
								},
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.drive.dev.ts"
								}								
							],							
							"optimization": false,
							"sourceMap": false,
							"poll": 2500,
							"verbose": false,
							"watch": true
						}												
					}
				},
				"playground": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/Drive",
						"baseHref": "",
						"fileReplacements": [
							{
								"replace": "src/app/website.ts",
								"with": "src/app/database/website-playground-dev.ts"
							},
							{
								"replace": "src/environments/environment.ts",
								"with": "src/environments/environment.drive.dev.ts"
							}
						],
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": true,
						"assets": [],
						"styles": [
							"src/styles.css",
							"src/assets/styles/pikaday.css"
						],
						"scripts": [
							"backend/modernizr/modernizr.js",
							"backend/polyfills/object.js",
							"backend/polyfills/signaturePad.js",
							"src/assets/scripts/pikaday.js"
						]
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.drive.prod.ts"
								},
								{
									"replace": "src/app/website.ts",
									"with": "src/app/database/website-drive-prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"assets": []
						},
						"development": {
							"optimization": false,
							"sourceMap": true,
							"poll": 2500,
							"verbose": false,
							"watch": true
						}
					}
				},			
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "AngularApp:build:development"
					},
					"configurations": {
						"production": {
							"browserTarget": "AngularApp:build:production"
						},
						"drive": {
							"browserTarget": "AngularApp:drive:development",
							"port": 8000,
							"liveReload": true
						},
						"upload": {
							"browserTarget": "AngularApp:drive:upload",
							"port": 8000,
							"liveReload": true
						},	
						"folders": {
							"browserTarget": "AngularApp:drive:folders",
							"port": 8000,
							"liveReload": true
						},												
						"playground": {
							"browserTarget": "AngularApp:drive:playground",
							"port": 8000,
							"liveReload": true
						}						
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "AngularApp:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"assets": [
							"src/favicon.ico",
							"src/assets"
						],
						"styles": [
							"src/styles.css"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"tsconfig.app.json",
							"tsconfig.spec.json",
							"e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "e2e/protractor.conf.js",
						"devServerTarget": "AngularApp:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "AngularApp:serve:production"
						}
					}
				}
			}
		}
	},
	"defaultProject": "AngularApp",
	"cli": {
		"analytics": "6fac06b6-fb10-4fba-ab5c-c36f335cf9fb"
	}
}